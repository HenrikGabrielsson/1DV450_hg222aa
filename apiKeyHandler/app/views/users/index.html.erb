
<div id="menu">
  <%= link_to("Registrera dig",new_user_path) %>
  <%= link_to("Logga in som administratör",admins_path) %>
</div>

<h1>Logga in</h1>

  <%= form_tag login_path, method: "post" do %>
  
  <%= label_tag :email, "Email: " %>
  <%= text_field_tag :email %>
  
  <%= label_tag :password, "Lösenord: " %>
  <%= password_field_tag :password %>
  
  <%= submit_tag("Logga in", {class: "submit_button"}) %>
  
  <% end %> 
  
  
